{
    "name": "pipetower/pipetower-php-sdk",
    "description": "The PHP SDK for the Pipetower API",
    "keywords": ["pipetower", "automation", "api", "sdk"],
    "homepage": "https://pipetower.com",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "symfony/var-dumper": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Pipetower\\PhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
